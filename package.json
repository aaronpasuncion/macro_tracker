{
  "name": "macro_tracker",
  "version": "1.0.0",
  "description": "calorie and macro tracker",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass scss/main.scss  dist/css/main.css -w",
    "compile:sass": "node-sass scss/main.scss dist/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" dist/css/style.comp.css -o dist/css/style.prefix.css",
    "compress:css": "node-sass dist/css/style.prefix.css dist/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "dev:js": "webpack --mode development",
    "build:js": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "Macro-Tracker"
  },
  "keywords": [
    "macro-tracker",
    "macro",
    "tracker",
    "calories",
    "calorie",
    "counter",
    "fitness",
    "weight",
    "loss",
    "exercise",
    "bodybuilding",
    "vanilla",
    "js",
    "html",
    "html5",
    "css",
    "css3",
    "BEM"
  ],
  "author": "Aaron Asuncion",
  "license": "ISC",
  "dependencies": {
    "core-js": "^3.6.5",
    "node-sass": "^4.14.1",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^7.2.6",
    "babel-loader": "^8.3.0",
    "concat": "^1.0.3",
    "html-webpack-plugin": "^5.5.0",
    "install": "^0.13.0",
    "npm": "^6.14.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.11.0"
  }
}
